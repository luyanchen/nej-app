#third-page input{display:none;}
#second-page,#third-page,#error,#success{display: none;}

.u-label{width: 100%;padding:10px 12px;font-size: 14px;color: #999999; }
.u-code{font-size: 16px;line-height: 40px;width:100px;padding: 0px 1px;}
$gray = #CCCCCC;
$green = #67C2C6;

.select-item,.u-code{
	vertical-align: middle;
	.active{color:$green;};
	.default{color:$gray;};
}
